{
  "name": "frontend",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "serve-local": "env NODE_ENV=test-local vue-cli-service serve",
    "serve-remote": "env NODE_ENV=test-remote vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "bootstrap-vue": "^2.15.0",
    "eslint": "^7.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "font-awesome": "4.7.0",
    "highlightjs": "^9.16.2",
    "liquor-tree": "^0.2.70",
    "vue": "^2.6.11",
    "vue-country-flag": "^2.0.1",
    "vue-gravatar": "^1.3.1",
    "vue-i18n": "^8.18.2",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.3.4",
    "vue-toasted": "^1.1.28",
    "vue2-editor": "^2.10.2",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
